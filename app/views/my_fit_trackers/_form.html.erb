<div class="row">
  <div class="col s10 offset-s1">
    <div class="row">
      <div class="input-field col s6">
        <i class="material-icons prefix">textsms</i>
        <input type="text" id="exercise" class="autocomplete">
        <label for="autocomplete-input">Autocomplete</label>
      </div>
      <div class="input-field col s3">
        <i class="material-icons prefix">label_outline</i>
        <input type="text" id="value" class="">
        <label for="value">Value</label>
      </div>
      <div class="input-field col s3">
        <i class="material-icons prefix">settings</i>
        <input type="text" id="um" class="">
        <label for="um">Measure unit</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12">
        <i class="material-icons prefix">settings_applications</i>
        <input type="text" id="exercise_type" class="">
        <label for="exercise_type">Excercise type</label>
      </div>
      <button id="submit" data-user-id="<%= current_user.id %>" class="btn waves-effect waves-light" type="submit" name="action" >Submit
		    <i class="material-icons right">send</i>
		  </button>
    </div>
  </div>
</div>

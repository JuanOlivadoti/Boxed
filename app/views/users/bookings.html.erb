<section class="fullwidth trainclass">
  <div class="container">

    <p id="notice"><%= notice %></p>

    <h1>Bookings</h1>

    <table>
      <thead>
        <tr>
          <th>User</th>
          <th>Trainclass</th>
        </tr>
      </thead>
      <tbody>
        <% @bookings.each do |booking| %>
          <tr id="booking-<%= booking.id %>">
            <td><%= booking.user_id %></td>
            <td><%= booking.trainclass_id %></td>
            
            <td><button id="js-burn" data-b-id="<%=booking.id%>" data-u-id="<%=current_user.id%>" data-t-id="<%=booking.trainclass_id%>">Burn!!</button></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</section>
    <h3>My fit tracker</h3>

			<div id="form">
				<%= render "my_fit_trackers/form" %>
			</div>
	</div>
	<div class="container row">
		<div class="input-field col s8 offset-s2">
      <p class="flow-text">Select your exercise list from Autocomplete</p>

<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<div class="row">
        <div class="col s5 offset-s1">
          <ul class="collection">
            <li class="collection-item">
              <p>User</p>
            </li>
          </ul>
        </div>
</div>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Trainclass</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.user_id %></td>
        <td><%= booking.trainclass_id %></td>
        <td><%= link_to 'Show', "/trainclasses/#{booking.trainclass_id}/booking/#{booking.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>